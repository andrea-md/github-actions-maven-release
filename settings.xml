<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                          https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <!-- This id must match the distributionManagement section in pom.xml -->
            <id>${env.MAVEN_REPO_SERVER_ID}</id>
            <privateKey>${env.MAVEN_REPO_PRIVATE_KEY}</privateKey>
            <passphrase>${env.MAVEN_REPO_PRIVATE_KEY_PASSPHRASE}</passphrase>
        </server>
    </servers>
</settings>